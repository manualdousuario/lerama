{
    "name": "lerama/feed-aggregator",
    "description": "A feed aggregator application supporting multiple feed formats",
    "type": "project",
    "require": {
        "php": "^8.4",
        "vlucas/phpdotenv": "^5.5",
        "league/climate": "^3.8",
        "league/route": "^5.1",
        "simplepie/simplepie": "^1.8",
        "sergeytsalkov/meekrodb": "^2.4",
        "laminas/laminas-diactoros": "^3.0",
        "laminas/laminas-httphandlerrunner": "^2.5",
        "league/plates": "^3.5",
        "guzzlehttp/guzzle": "^7.9",
        "phpmailer/phpmailer": "^6.8",
        "gregwar/captcha": "1.3.0"
    },
    "autoload": {
        "psr-4": {
            "Lerama\\": "src/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
